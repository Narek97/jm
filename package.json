{
  "name": "jm-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "lint": "yarn tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watchAll",
    "test:coverage": "vitest --coverage --watch=false",
    "test:coverage:watch": "vitest --coverage --watchAll"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
