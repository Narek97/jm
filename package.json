    "@eslint/js": "^9.22.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@npm-questionpro/wick-ui-eslint-config-frontend": "^0.0.3",
    "@npm-questionpro/wick-ui-icon": "^6.1.0",
    "@npm-questionpro/wick-ui-lib": "^0.19.1",
    "@npm-questionpro/wick-ui-prettier-config": "^1.0.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@tanstack/react-query": "^5.74.11",
    "@tanstack/react-query-devtools": "^5.74.11",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/router-devtools": "^1.119.1",
    "@tanstack/router-plugin": "^1.119.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
